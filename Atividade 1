#include <stdio.h>
#include <math.h>

main(){

    //Atividade 1
    float numero, resultado;

    printf("Digite um numero qualquer:");
    scanf("%f", &numero);

    if(numero>=0){

        resultado = sqrt(numero);
        printf("Raiz: %f", resultado);
    }else {
        resultado = pow(numero, 2);
        printf("Quadrado: %f", resultado);
    }


}
